    "@types/pg-pool": "^2.0.5",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "apidoc": "^1.2.0",
    "eslint": "^8.52.0",
    "nodemon": "^3.0.1",
    "tap": "^18.5.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
