{
  "name": "fastify-lambda",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "config": "cp ./.env.* ./dist/ && cp ./security/* ./dist/",
    "postinstall": "tsc",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && cross-env TS_NODE_FILES=true tap ./test",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "npm run config && tsc",
    "dev": "npm run config && tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"NODE_ENV=dev fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/sensible": "^5.5.0",
    "aws-lambda-fastify": "^2.2.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-cli": "^5.9.0",
    "fastify-plugin": "^4.5.1",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "pg-pool": "^3.6.1",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda-fastify": "^1.4.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "fastify-tsconfig": "^2.0.0",
    "i": "^0.3.7",
    "json-schema-to-ts": "^2.9.2",
    "npm": "^10.2.4",
    "tap": "^18.6.1",
    "ts-node": "^10.9.1"
  }
}
