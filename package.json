  "description": "L'atelier evaluation tmwong",
  "main": "src/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rm -r build/* && tsc -w",
    "dev": "NODE_ENV=dev nodemon build/server.js",
    "start": "node build/server.js",
    "eslint": "eslint ./src/**/*",
    "test": "tap \"test/**/*.test.js\"",
    "test:watch": "tap --watch \"test/**/*.test.js\"",
    "apidoc": "npx apidoc --debug -i 'routes/' -f '(index|root).js' -o apidoc"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tm-wong/l-atelier.git"
  },
  "engines": {
    "npm": "10.x",
    "node": "18.x"
  },
